    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    min-height: 100%;
    min-width: 100%;
    display: block;
    width: 100%;
    max-width: inherit;
  }

  @media screen and (max-width: 1199px) {
    .img-responsive-parent {
      width: 286.66px;
    }
  }

