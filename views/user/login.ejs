<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Registration Form</title>
  <link rel="stylesheet" href="../css/style.css">
  <!--[if lt IE 9]><script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
</head>

<body>
  <div class="container">
    <section class="register">
      <h1>Login HealthBot</h1>
      <form method="post" action="signin" >
      <%if(error) {%>
        <div class="reg_section personal_info">
        <h3>Please enter the information properly</h3>
        </div>
      <% } %>

      <div class="reg_section password" >
      <h3>Enter Login Credentials</h3>
      <input type="text" name="username" value="" placeholder="Enter Username" required/>
      <input type="password" name="password" value="" placeholder="Enter Password" required/>
      </div>
      <p class="submit"><input type="submit" name="commit" value="Log In"></p>
      </form>
    </section>
  </div>


</body>
</html>